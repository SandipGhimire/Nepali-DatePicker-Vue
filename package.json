{
    "name": "nepali-datepicker-vue",
    "private": true,
    "version": "1.0.12",
    "type": "module",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=24.0.0 <25.0.0"
    },
    "author": "Sandip Ghimire",
    "license": "MIT",
    "homepage": "https://nepalidate.sandip-ghimire.com.np",
    "repository": {
        "type": "github",
        "url": "https://github.com/SandipGhimire/Nepali-DatePicker-Vue"
    },
    "description": "A Vue 3 Nepali Datepicker with TypeScript Support",
    "main": "./dist/index.umd.cjs",
    "module": "./dist/index.es.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.cjs"
        },
        "./main.css": "./dist/nepali-datepicker-vue.css"
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "prebuild": "pnpm run lint",
        "dev": "vite",
        "build": "vue-tsc -b && vite build",
        "preview": "vite preview",
        "lint": "eslint src --max-warnings 0 --flag unstable_native_nodejs_ts_config",
        "lint:fix": "eslint src --max-warnings 0 --fix --flag unstable_native_nodejs_ts_config",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "nepali-date-library": "^1.1.12"
    },
    "peerDependencies": {
        "vue": "^3.5.27"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/node": "^25.0.10",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "@vitejs/plugin-vue": "^6.0.3",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/tsconfig": "^0.8.1",
        "eslint": "^9.39.2",
        "eslint-plugin-prettier": "^5.5.5",
        "eslint-plugin-vue": "^10.7.0",
        "path": "^0.12.7",
        "prettier": "^3.8.1",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.54.0",
        "vite": "^7.3.1",
        "vite-plugin-dts": "^4.5.4",
        "vue-tsc": "^3.2.4"
    },
    "keywords": [
        "vue3",
        "vue",
        "nepalidatepicker",
        "v-nepalidatepicker",
        "vue-datepicker",
        "datepicker",
        "nepali",
        "nepal",
        "nepalidate",
        "vuenepali",
        "vue nepali datepicker",
        "datepicker nepali",
        "vue nepali",
        "nepali-date",
        "nepali-calendar",
        "bikram-sambat",
        "bikram-sambat-date",
        "vue-nepali-datepicker",
        "vue-nepali-date-picker",
        "typescript",
        "javascript",
        "browser",
        "BS date"
    ]
}
